@using System.Security.Claims
@model VRSMODEL.DTO.MANAGE_COURSE_MASTER_AM_DTO
@{
    ViewData["Title"] = "Course";
    ViewData["ActiveMenu"] = "course_master";

}
<div class="content-wrapper">

    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    @*  <h1 class="m-0">Manage Course Master </h1> *@
                </div>
                @* <div class="col-sm-6">
                    <span class=" float-sm-right">
                        <a href="/admin/dashboard/course_master" class="btn  btn-outline-warning">Back To List</a>
                    </span>
                </div> *@
            </div>
        </div>
    </div>


    <section class="content">
        <div class="container-fluid">
            <div class="row">

                <div class="col-md-12">
                    <div class="card card-info">
                        <div class="card-header">
                            <h3 class="card-title">Manage Course Master</h3>
                            <span class=" float-sm-right">
                                <a href="/admin/dashboard/course_master" class="btn  btn-outline-warning">Back To List</a>
                            </span>
                        </div>


                        <div class="row">
                            <div class="col-md-6">
                                <div class="card-body">
                                    <div class="form-group row">
                                        <label for="inputCOURSE_ID_MAIN" class="col-sm-3 col-form-label">COURSE_ID MAIN</label>
                                        <div class="col-sm-9">
                                            <div class="form-group">

                                                <select class="form-control select2" style="width: 100%;" id="COURSE_ID_MAIN" onchange="Getchangeincourse()">
                                                    <option value="">--Select Course Main-- </option>
                                                    @if (Model.COURSE_MASTER_MAIN_LIST.Count() > 0)
                                                    {
                                                        foreach (var item in Model.COURSE_MASTER_MAIN_LIST)
                                                        {
                                                            <option value="@item.COURSE_ID_MAIN">@item.COURSE_NAME_MAIN </option>
                                                        }
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="inputCOURSE_NAME" class="col-sm-3 col-form-label">COURSE NAME</label>
                                        <div class="col-sm-9">
                                            <input type="text" class="form-control" id="COURSE_NAME" placeholder="COURSE_NAME" style="text-transform:uppercase">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="inputCOURSE_NAME_PROPER" class="col-sm-3 col-form-label">COURSE NAME PROPER</label>
                                        <div class="col-sm-9">
                                            <input type="text" class="form-control" id="COURSE_NAME_PROPER" placeholder="COURSE_NAME_PROPER">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="selectsemester" class="col-sm-3 col-form-label">SEMESTER</label>
                                        <div class="col-sm-9">
                                            <select class="form-control" id="selectsemester">
                                                <option value="">--Select--</option>
                                                <option value="1">1</option>
                                                <option value="2">2</option>
                                                <option value="3">3</option>
                                                <option value="4">4</option>
                                                <option value="5">5</option>
                                                <option value="6">6</option>
                                                <option value="7">7</option>
                                                <option value="8">8</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="form-group row">
                                        <label for="selectyear" class="col-sm-3 col-form-label">YEAR</label>
                                        <div class="col-sm-9">
                                            <select class="form-control" id="selectyear">
                                                <option value="">--Select--</option>
                                                <option value="1">1</option>
                                                <option value="2">2</option>
                                                <option value="3">3</option>
                                                <option value="4">4</option>
                                                <option value="5">5</option>
                                                <option value="6">6</option>
                                                <option value="7">7</option>
                                                <option value="8">8</option>
                                            </select>
                                        </div>
                                    </div>


                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="card-body">
                                    <div class="form-group row">
                                        <label for="selectcoursemode" class="col-sm-3 col-form-label">COURSE MODE</label>
                                        <div class="col-sm-9">
                                            <select class="form-control" id="selectcoursemode">
                                                <option value="">--Select--</option>
                                                @if (Model.COURSE_MODE_LIST.Count() > 0)
                                                {
                                                    foreach (var item in Model.COURSE_MODE_LIST)
                                                    {
                                                        <option value="@item.COURSE_MODE">@item.COURSE_MODE </option>
                                                    }
                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="selectcoursetype" class="col-sm-3 col-form-label">COURSE TYPE</label>
                                        <div class="col-sm-9">
                                            <select class="form-control" id="selectcoursetype">
                                                <option value="">--Select--</option>
                                                @if (Model.COURSE_TYPE_MASTER_LIST.Count() > 0)
                                                {
                                                    foreach (var item in Model.COURSE_TYPE_MASTER_LIST)
                                                    {
                                                        <option value="@item.COURSE_TYPE">@item.COURSE_TYPE_NAME </option>
                                                    }
                                                }
                                            </select>
                                        </div>
                                    </div>


                                    <div class="form-group row">
                                        <div class="offset-sm-2 col-sm-10">
                                            <div class="icheck-success d-inline">
                                                <input type="checkbox" id="IS_ACTIVE">
                                                <label for="IS_ACTIVE">
                                                    IS ACTIVE
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                   @*  <div class="form-group row">
                                        <div class="offset-sm-2 col-sm-10">
                                            <div class="icheck-success d-inline">
                                                <input type="checkbox" id="IS_GROUP">
                                                <label for="IS_GROUP">
                                                    IS GROUP
                                                </label>
                                            </div>

                                        </div>
                                    </div> *@
                                    <div class="form-group row">
                                        <div class="offset-sm-2 col-sm-10">
                                            <div class="icheck-success d-inline">
                                                <input type="checkbox" id="IS_NEP">
                                                <label for="IS_NEP">
                                                    IS NEP
                                                </label>
                                            </div>

                                        </div>
                                    </div>

                                </div>
                            </div>


                        </div>  <div class="card-footer">
                            <a id="btnc" onclick="SubmitFormC()" class="btn btn-info">Save</a>
                            <a class="btn btn-info" id="btnwaitc" onclick="SubmitFormC()" style="display:none">
                                <i class="fa fa-spinner fa-spin" aria-hidden="true"></i> Sending...</a>
                            <a href="~/admin/dashboard/managecoursemaster" class="btn btn-default float-right">Cancel</a>
                        </div>
                    </div>

                </div>




            </div>

        </div>
    </section>

</div>
@* <script src="~/pageui/plugins/jquery/jquery.min.js"></script> *@
<script src="~/js/AdminJs/CourseMaster.js" asp-append-version="true"></script>
