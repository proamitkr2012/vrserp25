@using System.Security.Claims
@model VRSMODEL.DTO.COURSE_MASTER_AM_DTO_DASH
@{
    ViewData["Title"] = "FORM";
    ViewData["ActiveMenu"] = "Correction_Form";
    ViewData["ActiveGroup"] = "EditMenu";
    var data = Model.CourseList;
}
<div class="content-wrapper">

    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                @*  <div class="col-sm-6">
                    <h1 class="m-0">Course Master </h1>
                </div> *@
                @* <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Dashboard </li>
                    </ol>
                </div> *@
            </div>
        </div>
    </div>


    <section class="content">
        <div class="container-fluid">
            <form id="formIndex" title="" method="post">
                <div class="row">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Course Master <a class="btn btn-xs btn-outline-success mx-3" href="~/admin/dashboard/managecoursemaster"> <i class="fas fa-plus"></i> Add New</a></h3>
                            <div class="card-tools">
                                <div class="input-group input-group-sm" style="width: 250px;">
                                    <input type="text" id="Search" name="Search" class="form-control float-right" placeholder="Search" value="@ViewBag.Search">
                                    <div class="input-group-append">
                                        <button type="submit" class="btn btn-default" id="SearchBtn">
                                            <i class="fas fa-search"></i>
                                        </button>
                                    </div>
                                </div>

                            </div>
                        </div>
                        @Html.Hidden("Page")
                        <div class="card-body table-responsive p-0">
                            <table class="table table-head-fixed text-nowrap">
                                <thead>
                                    <tr>
                                        <th scope="col">#</th>
                                        <th scope="col">COURSE_NAME</th>
                                        <th scope="col">COURSE_NAME_PROPER</th>
                                        <th scope="col">SEM_NO</th>
                                        <th scope="col">YEAR_NO</th>
                                        <th scope="col">COURSE_TYPE</th>
                                        <th scope="col">COURSE_MODE</th>
                                        <th scope="col">IS_ACTIVE</th>
                                        <th scope="col">MARKSHEET_FORMAT_CODE</th>
                                        <th scope="col">IS_GROUP</th>
                                        <th scope="col">IS_NEP</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (data != null)
                                    {
                                        if (data.Count() > 0)
                                        {

                                            foreach (var item in data)
                                            {

                                                <tr style="font-size:13px;">
                                                    <td>
                                                        @item.SrNo

                                                    </td>
                                                    <td>
                                                        @item.COURSE_NAME

                                                    </td>
                                                    <td>
                                                        @item.COURSE_NAME_PROPER

                                                    </td>
                                                    <td>
                                                        @item.SEM_NO

                                                    </td>
                                                    <td>
                                                        @item.YEAR_NO

                                                    </td>
                                                    <td>
                                                        @item.COURSE_TYPE

                                                    </td>
                                                    <td>
                                                        @item.COURSE_MODE

                                                    </td>
                                                    <td>
                                                        @item.IS_ACTIVE

                                                    </td>
                                                    <td>
                                                        @item.MARKSHEET_FORMAT_CODE

                                                    </td>
                                                    <td>
                                                        @item.IS_GROUP

                                                    </td>
                                                    <td>
                                                        @item.IS_NEP

                                                    </td>
                                                </tr>

                                            }
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                        <div class="card-footer clearfix">

                            
                        </div>
                    </div>
                </div>
            </form>


        </div>
    </section>

</div>

<script>
    function pagechange(p) {
        
        $("#Page").val(p);
        $("#formIndex").submit();
    }
</script>