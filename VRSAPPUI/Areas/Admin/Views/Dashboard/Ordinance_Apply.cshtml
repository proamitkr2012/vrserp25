@using System.Security.Claims
@model VRSMODEL.DTO.ORDINANCE_APPLY_AM_DTO_DASH
@{
    ViewData["Title"] = "ORDINANCE";
    ViewData["ActiveMenu"] = "ordinance_apply";
    ViewData["ActiveGroup"] = "ordinance";
}
<div class="content-wrapper">

    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    @*  <h1 class="m-0">Manage Course Master </h1> *@
                </div>
                @* <div class="col-sm-6">
                    <span class=" float-sm-right">
                        <a href="/admin/dashboard/course_master" class="btn  btn-outline-warning">Back To List</a>
                    </span>
                </div> *@
            </div>
        </div>
    </div>

    <form>
        <section class="content">
            <div class="container-fluid">
                <div class="row">

                    <div class="col-md-12">
                        <div class="card card-info">
                            <div class="card-header">
                                <h3 class="card-title">Ordinance Apply</h3>
                                <span class=" float-sm-right">
                                    @* <a href="/admin/dashboard/ordinance_master" class="btn  btn-outline-warning">Back To List</a> *@
                                </span>
                            </div>


                            <div class="row">
                                <div class="col-md-6">
                                    <div class="card-body">
                                        <div class="form-group row py-1">
                                            <label for="selectSESSION_ID" class="col-sm-3 col-form-label">SESSION</label>
                                            <div class="col-sm-9">
                                                <select class="form-control " style="width: 100%;" id="SESSION_ID" name="SESSION_ID">
                                                    <option value="">--Select Session-- </option>
                                                    @if (Model.SESSION_MASTER_LIST.Count() > 0)
                                                    {
                                                        foreach (var item in Model.SESSION_MASTER_LIST)
                                                        {
                                                            if (ViewBag.SESSION_ID == item.SESSIONS)
                                                            {
                                                                <option value="@item.SESSIONS" selected>@item.SESSIONS </option>
                                                            }
                                                            else
                                                            {
                                                                <option value="@item.SESSIONS">@item.SESSIONS </option>
                                                            }
                                                        }
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group row ">
                                            <label for="inputPAPER_NAME" class="col-sm-3 col-form-label">COURSE</label>
                                            <div class="col-sm-9">
                                                <select class="form-control select2" style="width: 100%;" id="COURSE_ID" name="COURSE_ID">
                                                    <option value="">--Select Course type-- </option>
                                                    @if (Model.COURSE_FILTER_LIST.Count() > 0)
                                                    {
                                                        foreach (var item in Model.COURSE_FILTER_LIST)
                                                        {
                                                            if (ViewBag.COURSE_ID == item.COURSE_ID)
        {
                                                                    <option value="@item.COURSE_ID" selected>@item.COURSE_NAME </option>
                                                                }
                                                                else
                                                                {   <option value="@item.COURSE_ID">@item.COURSE_NAME </option>
                                                                    
                                                                }
                                                            }
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label for="selectRESULT_TYPE" class="col-sm-3 col-form-label"></label>
                                            <div class="col-sm-9">
                                                <div class="icheck-success d-inline">

                                                    <input type="checkbox" id="IS_RW" name="IS_RW" @ViewBag.IsRWStatus onChange="this.form.submit()">
                                                    <label for="IS_RW">
                                                        IS_RW
                                                    </label>
                                                </div>
                                            </div>
                                        </div>


                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="card-body">

                                        <div class="form-group row py-1">

                                            <label for="selectRESULT_TYPE" class="col-sm-3 col-form-label">EXAM TYPE</label>
                                            <div class="col-sm-9">
                                                <select class="form-control " style="width: 100%;" id="EXAM_TYPE_ID" name="EXAM_TYPE_ID">
                                                    <option value="">--Select Exam type-- </option>
                                                    @if (Model.EXAM_TYPE_LIST.Count() > 0)
                                                    {
                                                        foreach (var item in Model.EXAM_TYPE_LIST)
                                                        {
                                                            if (ViewBag.EXAM_TYPE_ID == item.EXAM_TYPE)
                                                            {
                                                            <option value="@item.EXAM_TYPE" selected>@item.EXAM_TYPE </option>}
                                                            else
                                                            {
                                                                <option value="@item.EXAM_TYPE">@item.EXAM_TYPE </option>
                                                                }
                                                        }
                                                    }
                                                </select>
                                            </div>

                                        </div>
                                        <div class="form-group row py-1">

                                            <label for="selectRESULT_TYPE" class="col-sm-3 col-form-label">RESULT TYPE</label>
                                            <div class="col-sm-9">
                                                <select class="form-control " style="width: 100%;" id="RESULT_TYPE_ID" name="RESULT_TYPE_ID">
                                                    <option value="">--Select RESULT type-- </option>
                                                    @if (Model.RESULT_TYPE_LIST.Count() > 0)
                                                    {
                                                        foreach (var item in Model.RESULT_TYPE_LIST)
                                                        {
                                                            if (ViewBag.RESULT_TYPE_ID == item.RESULT_TYPE_ID)
                                                            {
                                                                <option value="@item.RESULT_TYPE_ID" selected>@item.RESULT_TYPE </option>
                                                            }
                                                            else
                                                            {
                                                                <option value="@item.RESULT_TYPE_ID">@item.RESULT_TYPE </option>
                                                            }
                                                        }
                                                    }
                                                </select>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="col-sm-12">
                                <div class="card-body">
                                    <div class="form-group row ">
                                        <label for="selectRESULT_TYPE" class="col-sm-2 col-form-label">Colleges</label>
                                        <div class="col-sm-10 ">
                                            <select class="form-control select2" multiple="multiple" style="width: 100%;" id="CollegeCodeDDL">
                                                <option value="" disabled>--Select  Colleges-- </option>
                                                @if (Model.COLLEGES_LIST.Count() > 0)
                                                {
                                                    foreach (var item in Model.COLLEGES_LIST)
                                                    {
                                                        <option value="@item.CollegeCode" data-rw="@item.IsRWCollege">[@item.CollegeCode] @item.CollegeName </option>
                                                    }
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="col-md-12">
                                <div class="card-body">
                                    <div class="form-group row">
                                        <label for="inputPROC_NAME" class="col-sm-2 col-form-label">ROLL NO</label>
                                        <div class="col-sm-10">
                                            <div class="form-group">
                                                <input type="text" class="form-control" id="ROLL_NO" placeholder="ROLL NO" style="text-transform:uppercase">

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card-footer">
                                <a id="btnc" onclick="SubmitFormA()" class="btn btn-danger">Click To Apply Rule</a>
                                <a class="btn btn-info" id="btnwaitc" style="display:none">
                                    <i class="fa fa-spinner fa-spin" aria-hidden="true"></i> Sending...
                                </a>
                                <a href="~/admin/dashboard/ordinance_apply" class="btn btn-default float-right">Refresh</a>
                            </div>
                        </div>

                    </div>




                </div>

            </div>
        </section>
    </form>
</div>
@* <script src="~/pageui/plugins/jquery/jquery.min.js"></script> *@
<script src="~/js/AdminJs/OrdinanceApply.js" asp-append-version="true"></script>
